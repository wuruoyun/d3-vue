{
  "name": "d3-vue",
  "version": "0.1.0",
  "description": "A Vue component library project",
  "author": "wuruoyun <wuruoyun@gmail.com>",
  "main": "dist/d3-vue.js",
  "dependencies": {
  },
  "peerDependencies": {
    "d3": "^5.5.0",
    "topojson": "^3.0.2",
    "vue": "^2.5.11"
  },
  "scripts": {
    "lint": "eslint --ext .js,.vue src test/unit/specs --fix",
    "lint-docs-vue": "eslint --plugin vue --ext .js,.vue docs/src --fix",
    "lint-docs-md-tag": "eslint --plugin script-tags --ext .js,.vue,.md docs/src --rule 'eol-last':0 --fix",
    "lint-docs": "npm run lint-docs-vue && npm run lint-docs-md-tag",
    "build": "cross-env NODE_ENV=production rollup -c",
    "dev": "rollup -c -w",
    "unit": "cross-env NODE_ENV=test jest --config test/unit/jest.conf.js",
    "test": "npm run unit",
    "start": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build-docs": "npm run clean-docs && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "clean-docs": "rimraf docs/dist"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.16",
    "autoprefixer": "^8.5.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^22.4.4",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "d3": "^5.5.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-script-tags": "^0.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.4",
    "jest-serializer-vue": "^1.0.0",
    "postcss-loader": "^2.0.8",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-plugin-vue": "^3.0.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "topojson": "^3.0.2",
    "uglify-es": "^3.3.9",
    "vue": "^2.5.11",
    "vue-jest": "^2.6.0",
    "vue-loader": "^15.2.0",
    "vue-md-loader": "^1.0.0",
    "vue-router": "^3.0.1",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.11",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.2"
  },
  "files": [
    "dist"
  ]
}
